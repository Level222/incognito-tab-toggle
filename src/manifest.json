{
  "manifest_version": 3,
  "name": "Incognito Tab Toggle",
  "version": "1.0.0",
  "description": "",
  "icons": {
    "16": "icons/icon-normal-16.png",
    "32": "icons/icon-normal-32.png",
    "48": "icons/icon-normal-48.png",
    "128": "icons/icon-normal-128.png"
  },
  "permissions": [
    "tabs",
    "tabGroups",
    "offscreen"
  ],
  "action": {
    "default_icon": {
      "16": "icons/icon-placeholder-16.png",
      "24": "icons/icon-placeholder-24.png",
      "32": "icons/icon-placeholder-32.png"
    },
    "default_title": "Toggle tab incognito"
  },
  "background": {
    "service_worker": "background.bundle.js"
  }
}
